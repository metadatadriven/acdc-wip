{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Thunderstruck",
  "patterns": [
    { "include": "#comments" },
    { "include": "#keywords" },
    { "include": "#model-keywords" },
    { "include": "#concept-types" },
    { "include": "#functions" },
    { "include": "#types" },
    { "include": "#constants" },
    { "include": "#strings" },
    { "include": "#numbers" },
    { "include": "#operators" }
  ],
  "repository": {
    "comments": {
      "patterns": [
        {
          "name": "comment.line.double-slash.thunderstruck",
          "match": "//.*$"
        },
        {
          "name": "comment.block.thunderstruck",
          "begin": "/\\*",
          "end": "\\*/"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "keyword.control.thunderstruck",
          "match": "\\b(import|as|from|where|depends|on)\\b"
        },
        {
          "name": "keyword.other.thunderstruck",
          "match": "\\b(cube|concept|slice|transform|model|aggregate|display|pipeline)\\b"
        },
        {
          "name": "keyword.other.structure.thunderstruck",
          "match": "\\b(namespace|structure|dimensions|measures|attributes|unit|codeLists)\\b"
        },
        {
          "name": "keyword.other.definition.thunderstruck",
          "match": "\\b(input|output|formula|family|link|random|subject|transformations)\\b"
        },
        {
          "name": "keyword.other.slice.thunderstruck",
          "match": "\\b(fix|vary)\\b"
        },
        {
          "name": "keyword.other.aggregate.thunderstruck",
          "match": "\\b(groupBy|statistics)\\b"
        },
        {
          "name": "keyword.other.display.thunderstruck",
          "match": "\\b(source|rows|columns|format|xAxis|yAxis|aesthetics|footnotes|table|figure)\\b"
        },
        {
          "name": "keyword.other.pipeline.thunderstruck",
          "match": "\\b(stages)\\b"
        },
        {
          "name": "keyword.other.concept.thunderstruck",
          "match": "\\b(type|category|definition)\\b"
        }
      ]
    },
    "model-keywords": {
      "patterns": [
        {
          "name": "support.class.model-family.thunderstruck",
          "match": "\\b(Gaussian|Binomial|Poisson|Gamma|InverseGaussian)\\b"
        },
        {
          "name": "support.class.link-function.thunderstruck",
          "match": "\\b(Identity|Log|Logit|Probit|Inverse|Sqrt)\\b"
        }
      ]
    },
    "concept-types": {
      "patterns": [
        {
          "name": "support.class.concept-type.thunderstruck",
          "match": "\\b(BiomedicalConcept|DerivationConcept|AnalysisConcept)\\b"
        }
      ]
    },
    "functions": {
      "patterns": [
        {
          "name": "entity.name.function.aggregate.thunderstruck",
          "match": "\\b(mean|median|stddev|variance|min|max|count|sum|quantile|ci_lower|ci_upper)\\b"
        }
      ]
    },
    "types": {
      "patterns": [
        {
          "name": "support.type.primitive.thunderstruck",
          "match": "\\b(Numeric|Integer|Text|DateTime|Date|Flag|Identifier|CodedValue)\\b"
        }
      ]
    },
    "constants": {
      "patterns": [
        {
          "name": "constant.language.boolean.thunderstruck",
          "match": "\\b(true|false)\\b"
        },
        {
          "name": "constant.language.null.thunderstruck",
          "match": "\\b(null|undefined)\\b"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.quoted.double.thunderstruck",
          "begin": "\"",
          "end": "\"",
          "patterns": [
            {
              "name": "constant.character.escape.thunderstruck",
              "match": "\\\\."
            }
          ]
        },
        {
          "name": "string.quoted.single.thunderstruck",
          "begin": "'",
          "end": "'",
          "patterns": [
            {
              "name": "constant.character.escape.thunderstruck",
              "match": "\\\\."
            }
          ]
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.scientific.thunderstruck",
          "match": "\\b\\d+(\\.\\d+)?[eE][+-]?\\d+\\b"
        },
        {
          "name": "constant.numeric.decimal.thunderstruck",
          "match": "\\b\\d+\\.\\d+\\b"
        },
        {
          "name": "constant.numeric.integer.thunderstruck",
          "match": "\\b\\d+\\b"
        }
      ]
    },
    "operators": {
      "patterns": [
        {
          "name": "keyword.operator.logical.thunderstruck",
          "match": "\\b(and|or|not)\\b"
        },
        {
          "name": "keyword.operator.comparison.thunderstruck",
          "match": "(==|!=|<=|>=|<|>)"
        },
        {
          "name": "keyword.operator.arithmetic.thunderstruck",
          "match": "(\\+|\\-|\\*|\\/|\\^)"
        },
        {
          "name": "keyword.operator.formula.thunderstruck",
          "match": "(~|:)"
        },
        {
          "name": "keyword.operator.assignment.thunderstruck",
          "match": "="
        },
        {
          "name": "punctuation.separator.thunderstruck",
          "match": "[,;]"
        },
        {
          "name": "punctuation.bracket.thunderstruck",
          "match": "[\\[\\]{}()]"
        },
        {
          "name": "punctuation.accessor.thunderstruck",
          "match": "\\."
        },
        {
          "name": "keyword.operator.pipe.thunderstruck",
          "match": "\\|"
        }
      ]
    }
  },
  "scopeName": "source.thunderstruck"
}
